<keymap version="1" name="Windows copy Proper Redo" parent="$default">
  <action id="$Redo">
    <keyboard-shortcut first-keystroke="shift ctrl z" />
    <keyboard-shortcut first-keystroke="shift alt back_space" />
    <keyboard-shortcut first-keystroke="ctrl y" />
  </action>
  <action id="ActivateLogcatToolWindow">
    <keyboard-shortcut first-keystroke="alt 6" />
  </action>
  <action id="ActivateTODOToolWindow" />
  <action id="CleanGradleProject">
    <keyboard-shortcut first-keystroke="ctrl alt comma" />
  </action>
  <action id="CloseProject">
    <keyboard-shortcut first-keystroke="shift ctrl alt open_bracket" />
  </action>
  <action id="Debug">
    <keyboard-shortcut first-keystroke="shift f9" />
    <keyboard-shortcut first-keystroke="ctrl alt back_slash" />
  </action>
  <action id="EditorDeleteLine" />
  <action id="FileChooser.LightEditGotoOpenedFile">
    <keyboard-shortcut first-keystroke="shift ctrl s" />
  </action>
  <action id="GotoCustomRegion" />
  <action id="RebuildGradleProject">
    <keyboard-shortcut first-keystroke="ctrl alt period" />
  </action>
  <action id="RevealIn">
    <keyboard-shortcut first-keystroke="ctrl alt x" />
  </action>
  <action id="Run">
    <keyboard-shortcut first-keystroke="shift f10" />
    <keyboard-shortcut first-keystroke="ctrl alt slash" />
  </action>
  <action id="StopBackgroundProcesses">
    <keyboard-shortcut first-keystroke="shift ctrl f2" />
    <keyboard-shortcut first-keystroke="shift ctrl alt close_bracket" />
  </action>
  <action id="UpdateRunningApplication" />
  <action id="android.deploy.ApplyChanges">
    <keyboard-shortcut first-keystroke="ctrl f10" />
  </action>
  <action id="android.deploy.CodeSwap">
    <keyboard-shortcut first-keystroke="ctrl alt f10" />
  </action>
</keymap>